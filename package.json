{
  "name": "unplugin-conditional-compilation",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@7.1.1",
  "description": "Similar to taro framework conditional compilation",
  "license": "MIT",
  "homepage": "https://github.com/Binbiubiubiu/unplugin-conditional-compilation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Binbiubiubiu/unplugin-conditional-compilation.git"
  },
  "bugs": {
    "url": "https://github.com/Binbiubiubiu/unplugin-conditional-compilation/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "resolver"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "npm run test:build && vitest run",
    "test:build": "esno scripts/buildFixtures.ts"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "unplugin": "^0.10.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "chalk": "^5.1.2",
    "esbuild": "^0.15.13",
    "eslint": "^8.26.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.23.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
